{
  "name": "@i2labs/dns",
  "version": "1.0.1",
  "author": {
    "name": "Nolan Woods",
    "url": "https://i2labs.ca"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/innovate-invent/dns.git"
  },
  "main": "dist/index.js",
  "browser": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "keywords": [
    "nodejs",
    "browserify",
    "dns",
    "DoH",
    "RFC8484",
    "RFC1035"
  ],
  "files": [
    "dist/"
  ],
  "scripts": {
    "prebuild": "npm run clean && npm run lint",
    "build": "rollup -c",
    "build:test": "TARGET=test npm run build",
    "start": "CHROME_BIN=chromium TARGET=test karma start karma.conf.cjs",
    "lint": "tslint src/*.ts",
    "clean": "rm -rf dist",
    "test:compat": "ts-mocha -p tsconfig.node.json spec/nodejs.spec.ts",
    "test": "karma start --single-run --browsers ChromeHeadless karma.conf.cjs",
    "publish": "npm publish --access public"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^9.0.2",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.6.1",
    "@types/estree": "^1.0.0",
    "@tsconfig/node18": "^1.0.1",
    "chai": "^4.3.6",
    "mocha": "^10.0.0",
    "rollup": "^2.77.0",
    "ts-loader": "^9.3.1",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^4.7.4",
    "uglifyjs": "^2.4.11",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "mocha-headless-chrome": "^4.0.0",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "^3.1.1",
    "karma-mocha": "^2.0.1",
    "karma-chai": "^0.1.0",
    "karma-rollup-preprocessor": "^7.0.8",
    "karma-typescript": "^5.5.3",
    "karma-coverage": "^2.2.0"
  },
  "dependencies": {}
}
